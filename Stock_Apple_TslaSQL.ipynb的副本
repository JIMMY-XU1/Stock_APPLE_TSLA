   "cell_type": "code",
   "execution_count": 202,
   "metadata": {},
   "outputs": [],
   "source": [
    "import dash\n",
    "import dash_core_components as dcc\n",
    "import dash_html_components as html\n",
    "import pandas as pd\n",
    "import plotly.graph_objs as go\n",
    "import plotly.express as px"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 203,
   "metadata": {},
   "outputs": [],
   "source": [
    "external_stylesheets = ['https://codepen.io/chriddyp/pen/bWLwgP.css']\n",
    "\n",
    "app = dash.Dash(__name__, external_stylesheets=external_stylesheets)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 204,
   "metadata": {},
   "outputs": [],
   "source": [
    "Stock_list = ['Open','High','Low','Close_P','Adj_P','Change']\n",
    "Company_list = ['APPLE', 'TSLA']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 205,
   "metadata": {},
   "outputs": [],
   "source": [
    "app.layout = html.Div([\n",
    "    \n",
    "    html.Div([\n",
    "        html.Br(),\n",
    "        html.Label([''],style={'font-weight': 'bold', \"text-align\": \"center\"}),\n",
    "        \n",
    "        dcc.RadioItems(\n",
    "        id = 'Stock',\n",
    "        options = [{'label': i, 'value': i} for i in Stock_list],\n",
    "        value = Stock_list[0],\n",
    "        labelStyle = {'display': 'inline-block'}),\n",
    "        \n",
    "        \n",
    "        dcc.Dropdown(\n",
    "            id = 'Company',\n",
    "            options = [{'label': i, 'value':i} for i in Company_list],\n",
    "            value=Company_list[0])\n",
    "        \n",
    "        \n",
    "        \n",
    "\n",
    "    ],style = {'width':'100%','margin-left':'0%'}),\n",
    "    \n",
    "    html.Div([\n",
    "        dcc.Graph(id = 'our_serie')\n",
    "        \n",
    "    ], className = 'nine columns'),\n",
    "    \n",
    "    \n",
    "],style ={'display': 'inline-block','width':\"99%\"})\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 206,
   "metadata": {},
   "outputs": [],
   "source": [
    "@app.callback(\n",
    "    dash.dependencies.Output('our_serie','figure'),\n",
    "    [dash.dependencies.Input('Company','value'),\n",
    "    dash.dependencies.Input('Stock','value')])\n",
    "\n",
    "def build_graph(Company,Stock):\n",
    "\n",
    "      \n",
    "    if Company == Company_list[0]:\n",
    "        \n",
    "        \n",
    "        Stock_Apple = ['trade_date_APPLE','open_p_APPLE', 'high_p_APPLE','low_p_APPLE','close_p_APPLE','Adj_close_APPLE','change_APPLE']\n",
    "        df_A = join_Apple_TSLA[Stock_Apple]\n",
    "        df_A = df_A.rename(columns={'open_p_APPLE':'Open', 'high_p_APPLE':'High', 'low_p_APPLE':'Low', 'close_p_APPLE':'Close_P','Adj_close_APPLE':'Adj_P','change_APPLE':'Change'})\n",
    "        index = pd.to_datetime(df_A.trade_date_APPLE)\n",
    "        df_A.index =index\n",
    "\n",
    "            \n",
    "        fig = px.line(x=df_A['trade_date_APPLE'], y = df_A[Stock],height=600)\n",
    "        \n",
    "      \n",
    "    else:\n",
    "        \n",
    "        Stock_TSLA = ['trade_date_TSLA','open_p_TSLA', 'high_p_TSLA','low_p_TSLA','close_p_TSLA','Adj_close_TSLA','change_TSLA']\n",
    "        df_T = join_Apple_TSLA[Stock_TSLA]\n",
    "        df_T = df_T.rename(columns={'open_p_TSLA':'Open', 'high_p_TSLA':'High', 'low_p_TSLA':'Low', 'close_p_TSLA':'Close_P','Adj_close_TSLA':'Adj_P','change_TSLA':'Change'})\n",
    "        index = pd.to_datetime(df_T.trade_date_TSLA)\n",
    "        df_T.index = index\n",
    "        \n",
    "\n",
    "        fig = px.line(x=df_T['trade_date_TSLA'], y = df_T[Stock],height=600)\n",
    "        \n",
    "    \n",
    "        \n",
    "    fig.update_layout(yaxis={'title':'Dollar'},xaxis={'title':'Date'},\n",
    "                      title={'text':\"APPLE_PK_TSLA sur Stock_Price\",\n",
    "                      'font':{'size':18},'x':0.5,'xanchor':'center'})\n",
    "    return fig\n",
    "\n"
   ]
  },
