    "#Ligne_Risk_Lower_Price: < 60; 60-80; 80-100; > 100\n",
    "curseur = connexion.cursor()\n",
    "\n",
    "Apple_join_4=pd.read_sql_query(\"SELECT L.*,case when cast(Low_2 as float64) <60 then '< 60' when cast(Low_2 as float64) >=60 and cast(Low_2 as float64) <80 then '60_80' when cast(Low_2 as float64) >=80 and cast(Low_2 as float64) <100 then '80_100' else '> 100' end as Ligne_Risk_Lower_Price  FROM  AAPL_Montly_updates_2 as L \",connexion);\n",
    "\n",
    "Apple_join_4.head(n=4)"
   ]
