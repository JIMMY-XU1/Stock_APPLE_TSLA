    "cur.execute(\"\"\"CREATE    TABLE    TSLA_Stock_Day\n",
    "                                  (\n",
    "                                   trade_date_TSLA       DATE          ,\n",
    "                                   open_p_TSLA           NUMERIC          ,\n",
    "                                   high_p_TSLA           NUMERIC          ,\n",
    "                                   low_p_TSLA            NUMERIC          ,\n",
    "                                   close_p_TSLA          NUMERIC          ,\n",
    "                                   Adj_close_TSLA        NUMERIC          ,\n",
    "                                   change_TSLA           NUMERIC          \n",
    "                                \n",
    "                                    ) ; \"\"\")\n",
    "\n",
    "print(\"Table created successfully\")\n",
    "\n",
    "con.commit()"
   ]
