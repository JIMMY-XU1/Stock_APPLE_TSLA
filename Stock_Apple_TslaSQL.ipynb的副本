   ],
   "source": [
    "join_Apple_TSLA =pd.read_sql_query(\"SELECT * FROM Apple_Stock_D Left JOIN TSLA_Stock_Day ON Apple_Stock_D.trade_date_APPLE = TSLA_Stock_Day.trade_date_TSLA\", con);\n",
    "\n",
    "join_Apple_TSLA\n",
    "\n",
    "# ici, il faut faire attention au fait: le data d'APPLE: 2019/09/10 - 2020/09/10 (254),\n",
    "# mais, le data de TSLA: 2010/06/29 - 2020-12-24(2643), \n",
    "# Left JOIN connecte automatiquement les même parties des deux datas （Intersection ∩ ）,\n",
    "# NON NON: Union ∪ (les différentes parties remplacé par Nan)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 51,
   "metadata": {},
   "outputs": [],
   "source": [
    "f = lambda x: parse(x)\n",
    "T= lambda t: t.strftime('%Y/%m/%d')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 52,
   "metadata": {},
   "outputs": [],
   "source": [
    "join_Apple_TSLA['trade_date_APPLE'] = (join_Apple_TSLA['trade_date_APPLE'].apply(f)).apply(T)\n"
   ]
  },
  {
