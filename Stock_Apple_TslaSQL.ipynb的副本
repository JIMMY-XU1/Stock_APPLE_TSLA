   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Table created successfully\n"
     ]
    }
   ],
   "source": [
    "cur.execute(\"\"\"CREATE    TABLE    Apple_Stock_D\n",
    "                                  (\n",
    "                                   trade_date_APPLE       DATE          ,\n",
    "                                   open_p_APPLE           NUMERIC          ,\n",
    "                                   high_p_APPLE           NUMERIC          ,\n",
    "                                   low_p_APPLE            NUMERIC          ,\n",
    "                                   close_p_APPLE          NUMERIC          ,\n",
    "                                   Adj_close_APPLE        NUMERIC          ,\n",
    "                                   change_APPLE              NUMERIC          \n",
    "                                \n",
    "                                    ) ; \"\"\")\n",
    "\n",
    "print(\"Table created successfully\")\n",
    "\n",
    "con.commit()\n"
   ]
  },
