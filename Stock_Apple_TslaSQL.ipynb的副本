    "low_p_APPLE = dA['Low'].tolist()\n",
    "close_p_APPLE = dA['Close'].tolist()\n",
    "Adj_close_APPLE = dA['Adj Close'].tolist()\n",
    "change_APPLE = dA['Volume'].tolist()\n",
    "trade_date_APPLE[1]"
