   "source": [
    "#  ici, en utilisant ? de présenter Espace réservé ***\n",
    "\n",
    "count = 0\n",
    "for i in range(len(trade_date_APPLE)):\n",
    "    cur.execute(\"\"\"\n",
    "    INSERT INTO Apple_Stock_D (trade_date_APPLE, open_p_APPLE, high_p_APPLE, low_p_APPLE, close_p_APPLE, Adj_close_APPLE, change_APPLE)\n",
    "    VALUES( ?, ?, ?, ?, ?, ?, ?);\"\"\",\n",
    "                 \n",
    "                ((trade_date_APPLE[i],\n",
    "                  open_p_APPLE[i],\n",
    "                  high_p_APPLE[i],\n",
    "                  low_p_APPLE[i],\n",
    "                  close_p_APPLE[i],\n",
    "                  Adj_close_APPLE[i],\n",
    "                  change_APPLE[i])))\n",
    "    con.commit()\n",
    "    print(\"Insert {0} Ligne，Total {1} ligne\".format(count, len(trade_date_APPLE)))\n",
    "    count += 1 \n",
    "    count < 254"
   ]
  },
